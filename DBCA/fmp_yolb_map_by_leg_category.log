# psql
#    --command "DROP TABLE IF EXISTS YOLB_by_leg_category"
#    --command "
CREATE TABLE YOLB_by_leg_category AS (
    SELECT 
        history.geom AS geom, 
        fireseason(fih_date1) AS year, 
        conservation_estate(leg_category) FROM dbca_fire_history_dbca_060_lmu_history_by_leg_category AS history 
      JOIN dbca_fire_history_dbca_060 AS event 
      ON event.id = history.id[1] 
      GROUP BY history.geom, fireseason(fih_date1), conservation_estate(leg_category));"